{
    "name": "ttr-cli",
    "version": "1.0.14",
    "description": "项目初始化脚手架",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon ./index.ts",
        "serve": "ts-node ./index.ts",
        "tsc": "tsc",
        "build": "tsc && ts-node build/index.ts",
        "commit": "git add . && git-cz ",
        "update-version": "ts-node build/version.ts",
        "commit-version": "npm run update-version && git add . -f && git-cz",
        "prepare": "husky install",
        "release": "tsc && ts-node build/index.ts"
    },
    "bin": {
        "ttr-cli": "dist/index.js"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "keywords": [
        "cli",
        "ttr"
    ],
    "author": "eli",
    "license": "MIT",
    "nodemonConfig": {
        "ignore": [
            "package.json"
        ]
    },
    "dependencies": {
        "cac": "^6.7.14",
        "chalk": "^4.1.2",
        "inquirer": "^8.2.4",
        "cross-spawn": "^7.0.3",
        "fs-extra": "^10.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-angular": "^17.0.3",
        "@commitlint/cz-commitlint": "^17.0.3",
        "@types/cross-spawn": "^6.0.2",
        "@types/fs-extra": "^9.0.13",
        "@types/inquirer": "^9.0.2",
        "@types/node": "^18.11.0",
        "commitizen": "^4.2.4",
        "cz-customizable": "^6.9.0",
        "husky": "^8.0.1",
        "inquirer": "^8.0.0",
        "minimist": "^1.2.7",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}